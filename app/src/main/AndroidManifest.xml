<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.com.dovui">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/App">
        <activity android:name=".activity.DiemCaoActivity"></activity>
        <activity android:name=".activity.ChoiMoiActivity" />
        <activity android:name=".activity.LuatChoiActivity" />
        <activity
            android:name=".activity.HomeActivity"
            android:theme="@style/App" />
        <activity
            android:name=".activity.SplassActivity"
            android:theme="@style/App">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>